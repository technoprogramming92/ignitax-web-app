---
import CoreLayoutWhite from "../components/CoreLayout-white.astro";
import Partners from "../components/Partners.astro";
import Layout from "../layouts/Layout.astro";
import { Icon } from "astro-icon/components";
---

<Layout>
  <CoreLayoutWhite>
    <!--Hero Start -->
    <section
      class="hero-area hero-area-corporate position-relative z-index-one section-margin-bottom"
    >
      <div class="container container-semi-large">
        <div class="row justify-content-end">
          <div class="col-lg-6 col-xl-4">
            <div class="hero-corporate-info" data-lag="0.2" data-stagger="0.08">
              <p class="text-white mb-4 mb-lg-5">
                Get Best Quality Website starting from &#8377; <b
                  >5999/-
                </b>only
              </p>
              <p class="text-white mb-4" style="font-size: 1rem">
                Our Experienced professional team will craft your website that
                has Aesthetic look, Mobile Responsive, Fast Loading, Content
                Richness and Graphical Richness.
              </p>
              <a class="common-design-btn white-bg" href="contact.html">
                <span class="btn-flip">
                  <span data-text="Get Free Quotation">Get Free Quotation</span>
                </span>
              </a>
            </div>
          </div>
        </div>
        <div class="row section-margin-top">
          <div class="col-lg-12">
            <div class="hero-two-info" data-lag="0.2" data-stagger="0.08">
              <span
                class="text-white fnt-weight-500 hero-three-sub d-inline-block mb-1"
                >Grow business with</span
              >
              <h1 class="text-white text-uppercase">IGNITAX</h1>
              <p class="text-white mb-4" style="font-size: 1rem">
                #We Do not claim to be Ahmedabad's #1 web design companies
                because there are many number 1 companies are there.
              </p>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--Hero End -->

    <!--Partners Start -->

    <Partners />

    <!--Partners End -->

    <!--Core Services Start -->

    <section class="services-area section-padding-bottom">
      <div class="container">
        <div class="row align-items-lg-end gy-4">
          <div class="col-lg-9">
            <div class="section-title text-center text-lg-start">
              <h2 class="h2 mt-2 title-anim">List of Websites</h2>
            </div>
          </div>
        </div>
        <div class="row gy-4" style="margin-top: 8px;">
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 12px;">
              <div class="services-info">
                <h3 class="h3">Business Websites</h3>
                <p>
                  <ul>
                    <li>Corporate Website</li>
                    <li>Startup Website</li>
                    <li>Small Business Website</li>
                    <li>Business Portfolio Website</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 12px;">
              <div class="services-info">
                <h3 class="h3">E-Commerce Websites</h3>
                <p>
                  <ul>
                    <li>Online Store (Single Vendor)</li>
                    <li>Marketplace (Multi-Vendor)</li>
                    <li>Subscription-Based Store</li>
                    <li>Dropshipping Website</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 8px;">
              <div class="services-info">
                <h3 class="h3">Personal Websites</h3>
                <p>
                  <ul>
                    <li>Personal Blog</li>
                    <li>Resume/CV Website</li>
                    <li>Personal Branding Website</li>
                    <li>Portfolio Website</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
        </div>
        <div class="row gy-4" style="margin-top: 8px;">
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 12px;">
              <div class="services-info">
                <h3 class="h3">Informational Websites</h3>
                <p>
                  <ul>
                    <li>News Portal</li>
                    <li>Magazine Website</li>
                    <li>Educational Website</li>
                    <li>Wiki or Knowledge Base</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 12px;">
              <div class="services-info">
                <h3 class="h3">Entertainment Websites</h3>
                <p>
                  <ul>
                    <li>Streaming Website (Video/Audio)</li>
                    <li>Meme or Viral Content Website</li>
                    <li>Gaming Website</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 8px;">
              <div class="services-info">
                <h3 class="h3">Social Media & Community Websites</h3>
                <p>
                  <ul>
                    <li>Social Networking Platform</li>
                    <li>Forum or Discussion Board</li>
                    <li>Membership Website</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
        </div>
        <div class="row gy-4" style="margin-top: 8px;">
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 12px;">
              <div class="services-info">
                <h3 class="h3">Booking & Service Websites</h3>
                <p>
                  <ul>
                    <li>Hotel/Travel Booking Website</li>
                    <li>Appointment Scheduling Website</li>
                    <li>Event Booking Website</li>
                  </ul>
                </p>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6 fade_left">
            <div class="services-box" style="padding-top: 12px;">
              <div class="services-info">
                <h3 class="h3">Non-Profit & Government Websites</h3>
                <p>
                  <ul>
                    <li>Charity or Fundraising Website</li>
                    <li>NGO Website</li>
                    <li>Government Information Website</li>
                  </ul>
