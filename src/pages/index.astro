      class="hero-two-area section-padding-bottom bg_color_white overflow-hidden"
    >
      <div class="container container-semi-large">
        <div class="row gy-5">
          <div class="col-lg-12">
            <div class="hero-two-info">
              <h1 class="reveal_anim_after_loader">
                <span><h3>We are</h3></span>Team of Creators
              </h1>
            </div>
          </div>
          <div
            class="col-lg-12 hero-two-bottom-info hero-two-border position-relative"
          >
            <div class="row">
              <div class="col-lg-6 overflow-hidden order-1 order-lg-0">
                <div
                  class="hero-two-box h-100 d-flex align-items-center justify-content-center position-relative"
                >
                  <p class="h-box box-1">MARKETER</p>
                  <p class="h-box box-2">CODERS</p>
                  <p class="h-box box-3">DESIGNERS</p>
                </div>
              </div>
              <div
                class="col-lg-6 h-100 overflow-hidden about-two-cta-wrap position-relative"
